<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="489.0" prefWidth="729.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="employeemanagement.LoginController">
   <children>
      <ImageView blendMode="MULTIPLY" fitHeight="496.0" fitWidth="729.0" layoutY="1.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../photo/login.png" />
         </image>
      </ImageView>
      <Button fx:id="btnLogin" alignment="CENTER" blendMode="HARD_LIGHT" contentDisplay="CENTER" layoutX="164.0" layoutY="419.0" mnemonicParsing="false" onAction="#login" text="Login" textAlignment="CENTER">
         <font>
            <Font size="17.0" />
         </font>
      </Button>
      <TextField fx:id="txtCodeMeli" alignment="CENTER" blendMode="HARD_LIGHT" layoutX="56.0" layoutY="318.0" promptText="کد ملی">
         <font>
            <Font name="B Morvarid" size="17.0" />
         </font>
      </TextField>
      <PasswordField fx:id="txtPassword" alignment="CENTER" blendMode="HARD_LIGHT" layoutX="56.0" layoutY="363.0" prefHeight="34.0" prefWidth="172.0" promptText="رمز عبور">
         <font>
            <Font name="B Morvarid" size="17.0" />
         </font>
      </PasswordField>
   </children>
</AnchorPane>
